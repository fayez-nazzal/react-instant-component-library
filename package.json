{
  "name": "react-instant-component-library",
  "version": "1.0.0",
  "description": "A boilerplate for a component library using rollup, typescript, storybook, sass, jest, eslint, prettier.",
  "license": "MIT",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "testEnvironment": "jsdom",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "prettier": "prettier --write src/**/*.{js,tsx}",
    "eslint": "eslint ./**/*.{js,tsx}",
    "lint": "npm run test && npm run prettier && npm run eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@pandacss/dev": "^0.4.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@storybook/addon-actions": "^7.1.0-alpha.38",
    "@storybook/addon-essentials": "^7.1.0-alpha.38",
    "@storybook/addon-interactions": "^7.1.0-alpha.38",
    "@storybook/addon-links": "^7.1.0-alpha.38",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^7.1.0-alpha.38",
    "@storybook/react-webpack5": "^7.1.0-alpha.38",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/rollup": "^8.0.1",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.24",
    "pre-commit": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.25.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-url": "^3.0.1",
    "sass-loader": "^13.3.2",
    "storybook": "^7.1.0-alpha.38",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "5.88.0"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@storybook/cli": "^7.1.0-alpha.38"
  }
}
